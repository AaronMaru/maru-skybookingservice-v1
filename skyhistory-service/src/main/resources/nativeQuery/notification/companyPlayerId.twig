SELECT
	suhc.id,
	suhc.skyuser_role AS skyuserRole,
	up.player_id AS playerId
FROM stakeholder_user_has_companies suhc

INNER JOIN
(
	SELECT *
	FROM user_playid up
) up ON up.stuser_id = suhc.stakeholder_user_id

WHERE suhc.stakeholder_user_id = :skyuserId
AND suhc.stakeholder_company_id = :companyId